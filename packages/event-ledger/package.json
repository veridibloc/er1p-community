{
  "name": "@er1p-community/event-ledger",
  "version": "0.1.4",
  "description": "Isomorphic event ledger functionality for ER1P blockchain interactions",
  "type": "module",
  "private": "false",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veridibloc/er1p-community.git",
    "directory": "packages/event-ledger"
  },
  "keywords": [
    "er1p",
    "event",
    "ledger",
    "blockchain",
    "signum",
    "smartcontracts"
  ],
  "author": "ohager",
  "license": "MIT",
  "dependencies": {
    "valibot": "^1.1.0"
  },
  "peerDependencies": {
    "@signumjs/core": "^2.0.8",
    "@signumjs/crypto": "^2.0.8",
    "@signumjs/standards": "^2.0.8",
    "@signumjs/util": "^2.0.8"
  },
  "devDependencies": {
    "@signumjs/core": "^2.0.8",
    "@signumjs/crypto": "^2.0.8",
    "@signumjs/standards": "^2.0.8",
    "@signumjs/util": "^2.0.8",
    "@er1p-community/typescript-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "turbo": {
    "tasks": {
      "build": {
        "outputs": ["dist/**"]
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
